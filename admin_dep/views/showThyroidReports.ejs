  <div class="row justify-content-center">
    <% for(let history of ReportsArray) {%>
      <div id = "HistorySection" class="card border-info mb-3" style="max-width: 18rem;margin:17px;padding:0px">
          <div class="card-body">
            <h5 class="card-title"><%=history.name%></h5>
            <h6 class="card-subtitle mb-2 text-muted"><%=history.date%></h6>
            <%
            
            var result = "";
              var characters       = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz';
              var charactersLength = characters.length;
              for ( var i = 0; i < charactersLength; i++ ) {
                result+=(characters.charAt(Math.floor(Math.random() * charactersLength)));
              }
              
            %>

            <!-- <p><%= result %></p> -->

            <button class="btn btn-outline-primary" data-toggle="collapse" data-target="#<%= result %>">View Complete Details</button>
            <div id="<%= result %>" class="collapse">

              <form id = "classroom_add_id">
                <div class="mb-3">
                  <label for="formGroupExampleInput" class="form-label">Hospital</label>
                  <input readonly type="text" class="form-control" id="buildingname"
                      value="<%=history.hospitalText %>" required>
  
                      <label for="formGroupExampleInput" class="form-label">Unit</label>
                      <input readonly type="text" class="form-control" id="buildingname"
                          value="<%=history.unitText %>" required>
  
                          <label for="formGroupExampleInput" class="form-label">Alcohole</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.alcohole %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Comorbidities</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.comorbidities %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Doctor Remarks</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.doctorRemarks %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Family History</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.familyho %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Height</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.height %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Weight</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.weight %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Medicines</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.medicines %>" required>
  
                              <label for="formGroupExampleInput" class="form-label">Smoking H/O</label>
                          <input readonly type="text" class="form-control" id="buildingname"
                              value="<%=history.smoking %>" required>
  
                              <br>
                              <% if(history.mri=="") { %>
                              <a class="btn btn-success" href="#" target="_blank">MRI</a>
                              <% } %>
                              <% if(history.mri!="") { %>
                                <a class="btn btn-success" href=<%=history.mri %> target="_blank">MRI</a>
                                <% } %>
  
                                <% if(history.bonescan=="") { %>
                                  <a class="btn btn-success" href="#" target="_blank">BoneScan</a>
                                  <% } %>
                                  <% if(history.bonescan!="") { %>
                                    <a class="btn btn-success" href=<%=history.bonescan %> target="_blank">BoneScan</a>
                                    <% } %>
  
  
                                    <% if(history.psmapet=="") { %>
                                      <a class="btn btn-success" href="#" target="_blank">PSMA</a>
                                      <% } %>
                                      <% if(history.psmapet!="") { %>
                                        <a class="btn btn-success" href="<%=history.psmapet %>" target="_blank">PSMA</a>
                                        <% } %>
  
                            </div>
            </form>
  
              </div>
  
          </div>
    </div>
    <% } %>
  </div>


    
    
